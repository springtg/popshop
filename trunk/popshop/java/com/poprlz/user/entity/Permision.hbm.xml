<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.poprlz.user.entity">
	<class name="Permision" table="permision">
		<id name="permisionId" column="permisionId">
			<generator class="native" />
		</id>
		<property name="permisionName" not-null="true"
			column="permisionName" />
		<property name="permisionContent" not-null="true"
			column="permisionContent" />

		<property name="stutas" not-null="true" column="stutas" />

	</class>
</hibernate-mapping>