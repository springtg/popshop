<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.poprlz.user.entity">
	<class name="RoleInfo" table="roleInfo">
		<id name="roleId" column="roleId">
			<generator class="native" />
		</id>
		<property name="roleName" not-null="true" column="roleName" />
		<property name="roleContent" not-null="true"
			column="roleContent" />

		<property name="stutas" not-null="true" column="stutas" />

	</class>
</hibernate-mapping>