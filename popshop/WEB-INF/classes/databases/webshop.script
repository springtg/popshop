CREATE MEMORY TABLE SHOPCART(SESSIONID CHAR(100) NOT NULL PRIMARY KEY,USERINFOID INTEGER,USERNAME VARCHAR(100),CREATEDATE DATE,LASTACESSDATE DATE,CLIENTIP CHAR(100),TOTALPRICE DECIMAL(10,2))
CREATE MEMORY TABLE SHOPCARTPRODUCTITEM(PRODUCTITEMID INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 0) NOT NULL PRIMARY KEY,SESSIONID CHAR(100) NOT NULL,PRODUCTID INTEGER NOT NULL,PRODUCTNAME VARCHAR(100),PRICE DECIMAL(10,2),ORDERQUANTITY INTEGER,DESCRIPTION LONGVARCHAR,WEIGHT DECIMAL(10,2),URL VARCHAR(100),IMAGEID INTEGER,IMGPATH VARCHAR(100))
ALTER TABLE SHOPCARTPRODUCTITEM ALTER COLUMN PRODUCTITEMID RESTART WITH 9
